    <insert id="add" parameterType="com.cystrix.blog.entity.Cover" keyProperty="id" useGeneratedKeys="true">
        insert into cover(id, url, name, type, size) value (null, #{url}, #{name}, #{type}, #{size})
    </insert>
</mapper>